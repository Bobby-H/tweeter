<%= link_to 'Back', tweet_path %>
<p id="notice"><%= notice %></p>
<h1>Twitter Feed</h1>

<table>
  <thead>
    <tr>
      <th>Message</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @tweets.each do |tweet| %>
    <tr>
      <td><%= tweet.%></td>
      <td><%= tweet.message%></td>
      <td><%= tweet.user.username%></td>
      <td><%= link_to 'Show', tweet %></td>
      <td><%= tweet.%><%= link_to 'Edit', edit_tweet_path(tweet) %></td>
      <td><%= tweet.%><%= link_to 'Destroy', tweet, method: :delete, data: { confirm: 'Are You Sure?' } %></td>
    </tr>
  </tbody>
</table>
<%= link_to "New Tweet" %>
